{
    "name": "chargely/chargify-sdk-php",
    "homepage": "https://github.com/chargely/chargify-sdk-php",
    "description": "Chargify SDK for PHP - Use the Chargify API in your PHP project",
    "keywords": [
        "chargify",
        "chargify direct",
        "chargify v2",
        "billing portal",
        "api",
        "sdk",
        "php",
        "chargely"
    ],
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Chargely",
            "homepage": "http://www.chargely.com"
        }
    ],
    "support": {
        "issues": "https://github.com/chargely/chargify-sdk-php/issues"
    },
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "fabpot/php-cs-fixer": "^3.14",
        "monolog/monolog": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Crucial\\Service\\": "src/Crucial/Service"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Crucial\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "prefer-stable": true
}
